<h1>Recipes#index</h1>
<p>Find me in app/views/recipes/index.html.erb</p>

<% @recipe.each do |recipe| %>
					<tr>
						
						<td><%= link_to recipe.title, recipe, class: "recipe_#{recipe.id}" %></td>
